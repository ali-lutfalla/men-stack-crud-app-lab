<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cars Index</title>
</head>
<body>
    <a href="/cars/new">Add new car</a>
    <h1>All Cars</h1>
    <ul>
        <% cars.forEach((car) => { %>
            <li><a href="/cars/<%= car._id %>">
            <%= car.company %>
            <%= car.model %>
            <%= car.year %> 
        <% }) %></li></a>
    </ul>
    <a href="/">back to home</a>
</body>
</html>